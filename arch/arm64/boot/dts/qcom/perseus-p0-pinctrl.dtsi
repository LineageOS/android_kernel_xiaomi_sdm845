&nfc_enable_active {
	/* active state */
	mux {
		/* 128: NFC ENABLE 116:ESE Enable */
		pins = "gpio128", "gpio88", "gpio116";
		function = "gpio";
	};

	config {
		pins = "gpio128", "gpio88", "gpio116";
		drive-strength = <2>; /* 2 MA */
		bias-pull-up;
	};
};

&nfc_enable_suspend {
	/* sleep state */
	mux {
		/* 128: NFC ENABLE 116:ESE Enable */
		pins = "gpio128", "gpio88", "gpio116";
		function = "gpio";
	};

	config {
		pins = "gpio128", "gpio88", "gpio116";
		drive-strength = <2>; /* 2 MA */
		bias-disable;
	};
};

