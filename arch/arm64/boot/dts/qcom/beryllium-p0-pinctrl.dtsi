			bias-high-impedance; /* DISABLE GPIO8 for ADC*/
		};
	};
};

&tlmm {
	atest_usb13_active: atest_usb13_active {
		mux {
			pins = "gpio62";
			function = "gpio";
		};
		config {
			pins = "gpio62";
			drive-strength = <12>;
			output-high;
			bias-pull-up;
		};
	};

	atest_usb13_suspend: atest_usb13_suspend {
		mux {
			pins = "gpio62";
			function = "gpio";
		};
		config {
			pins = "gpio62";
			drive-strength = <2>;
			output-low;
			bias-pull-down;
		};
	};
};

