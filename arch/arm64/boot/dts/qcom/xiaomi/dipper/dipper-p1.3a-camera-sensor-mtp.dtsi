	compatible = "qcom,eeprom";
	slave-addr = <0x5a>;
	i2c-freq-mode = <1>;
	num-blocks = <1>;
	page0 = <0 0 0 0 0 0>;
	poll0 = <0 0 0 0 0 0>;
