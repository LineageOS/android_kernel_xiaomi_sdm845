#include "perseus-p1-pinctrl.dtsi"


&tlmm {

	/* GPIO_51 : FP_RESET_N */
	msm_gpio_51: msm_gpio_51 {
		mux {
			 pins = "gpio51";
			 function = "gpio";
		};

		config {
			pins = "gpio51";
			drive-strength = <2>;
			bias-disable;
			output-low;
		};
	};

	/* GPIO_51 : FP_RESET_N, state device active*/
	msm_gpio_51_output_high: msm_gpio_51_output_high {
		mux {
			pins = "gpio51";
			function = "gpio";
		};

		config {
			pins = "gpio51";
			drive-strength = <2>;
			bias-disable;
			output-high;
		};
	};
};
